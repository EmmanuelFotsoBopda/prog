#!/usr/bin/env python
# coding: utf-8

# In[ ]:


# Prompt user for input and convert to integer
score = int(input("Enter your score (0-100): "))

# Determine grade based on conditional logic
if score >= 90:
    grade = "A"
elif score >= 80:
    grade = "B"
elif score >= 70:
    grade = "C"
elif score >= 60:
    grade = "D"
else:
    grade = "F"

print(f"Your grade is: {grade}")

# Bonus: Achievement message
if grade == "A":
    print("Excellent work!")

